def numbers():
    for i in range(101):
        if i%3==0 and i%5==0:
            print (i "Bottles of Root Beer on the wall")
        elif i%3==0:
            print (i "Bottles of Root Beer on the wall")
        elif i%5==0:
            print (i "Bottles of Root Beer on the wall")
        else:
            print (i "Bottles of Root Beer on the wall")
